<p style="color: green"><%= notice %></p>

<h1><%= t('.title') %></h1>

<input class="w-full placeholder-red-500 placeholder-opacity-100" placeholder="I will look lighter in Firefox" />

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to t('.new'), new_user_path %>
